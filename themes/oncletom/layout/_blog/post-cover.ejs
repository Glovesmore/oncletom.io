<% if (post.cover){ %>
<div class="post-cover">
  <% if (post.cover.hasOwnProperty("link")){ -%><a href="<% if(typeof href !== 'undefined') { %><%= href %><% } else { %><%= post.cover.link %><% } %>" rel="external"><% } %>
